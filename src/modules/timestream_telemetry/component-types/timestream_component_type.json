{
    "componentTypeId": "com.example.timestream-telemetry",
    "functions": {
        "dataReader": {
            "implementedBy": {
                "lambda": {
                    "arn": "{lambdaArn}"
                }
            }
        }
    },
    "description": "Models a telemetry asset (i.e. data stream) stored in Timestream. Note: 'asset' here refers to the data stream not the physical asset. TelemetryAssetId uniquely identifies the data stream in the IoT TwinMaker-external system (Timestream in this case) hence it is marked as 'isExternalId=true'.",
    "propertyDefinitions": {
        "telemetryAssetType": {
            "dataType": {
                "type": "STRING"
            },
            "isExternalId": false,
            "isStoredExternally": false,
            "isTimeSeries": false,
            "isRequiredInEntity": true
        },
        "telemetryAssetId": {
            "dataType": {
                "type": "STRING"
            },
            "isExternalId": true,
            "isStoredExternally": false,
            "isTimeSeries": false,
            "isRequiredInEntity": true
        }
    }
}